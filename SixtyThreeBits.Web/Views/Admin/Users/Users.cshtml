@model UsersModel.PageViewModel
<div class="card">
    <div class="card-body">
        <div class="row">
            <div class="col">
                @if (Model.ShowAddNewButton)
                {
                    @await Html.PartialAsync(ViewNames.Admin.Shared.ButtonAddNewPartialView, new ButtonAddNewViewModel())
                }                
            </div>
        </div>
    </div>
</div>
<div class="card">
    <div class="card-body">        
        <div class="row">
            <div class="col">
                @Model.Grid.Render(Html)
            </div>
        </div>
    </div>
</div>
@section FooterSection{
    <script src="/js/admin/users/users.js"></script>
}