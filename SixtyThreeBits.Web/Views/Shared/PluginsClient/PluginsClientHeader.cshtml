@model PluginsClientViewModel
@if (Model.IsGoogleFontsEnabled)
{
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,300;0,400;0,500;0,700;1,400&display=swap" rel="stylesheet">
}
@if (Model.IsFontAwesomeEnabled)
{
    <link href="/plugins/fontawesome/css/all.css" rel="stylesheet" />
}
@if (Model.Is63BitsFontsEnabled)
{
    <link href="/plugins/63bits-fonts/fonts.css" rel="stylesheet" />
}
@if (Model.IsBootstrapEnabled)
{
    <link href="/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
}
@if (Model.IsAdminThemeEnabled)
{
    <link href="/plugins/borex/css/icons.min.css" rel="stylesheet" />
    <link href="/plugins/borex/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/plugins/borex/plugins/simplebar/simplebar.min.css" rel="stylesheet" />
    <link href="/plugins/borex/css/app.css" rel="stylesheet" />
}
@if (Model.IsJQueryConfirmEnabled)
{
    <link href="/plugins/jquery-confirm/jquery-confirm.min.css" rel="stylesheet" />
}
@if (Model.IsFancyboxEnabled)
{
    <link href="/plugins/fancybox/jquery.fancybox.css" rel="stylesheet" />
    <link href="/plugins/fancybox/style.css" rel="stylesheet" />
}
@if (Model.IsUtilsEnabled)
{
    <link href="/plugins/utils/utils.css" rel="stylesheet" />
}
@if (Model.Is63BitsFormsEnabled)
{
    <link href="/plugins/63bits-bsforms/63bits-bsforms.css" rel="stylesheet" />
}
@if(Model.IsMetisMenuEnabled)
{
    <link href="/plugins/metismenujs/metismenujs.min.css" rel="stylesheet" />
}
@if (Model.IsPreloaderEnabled)
{
    <link href="/plugins/preloader/preloader.css" rel="stylesheet" />
}
@if (Model.IsPageBuilderEnabled)
{
    <link href="/plugins/63bits-page-builder/css/template63.css" rel="stylesheet" />
}
@if (Model.IsPageBuilderEditorEnabled)
{
    <link href="/css/website/style.css" rel="stylesheet" />
    <link href="/plugins/63bits-page-builder/css/template63.css" rel="stylesheet" />
    <link href="/plugins/63bits-page-builder/css/page-builder.css" rel="stylesheet" />
}
@if (Model.IsFlatPickrEnabled)
{
    <link href="/plugins/flatpickr/flatpickr.min.css" rel="stylesheet" />
}
@if (Model.IsSlickSliderEnabled)
{
    <link href="/plugins/slick/slick.min.css" rel="stylesheet" />
}
@if (Model.IsMalihuScrollEnabled)
{
    <link href="/plugins/malihu-scrollbar/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
}
@if (Model.Is63BitsSuccessErrorToastEnabled)
{
    <link href="/plugins/63bits-success-error-toast/63bits-success-error-toast.css" rel="stylesheet" />
}

@if (Model.IsJQueryEnabled)
{
    <script src="~/plugins/jquery/jquery.min.js"></script>
}
@if (Model.IsDevextremeEnabled)
{    
    <link href="/plugins/devextreme/css/dx.light.compact.css" rel="stylesheet" />
    <link href="/css/admin/devexpress.css" rel="stylesheet" />
    <script src="/plugins/devextreme/js/jszip.js"></script>
    <script src="/plugins/devextreme/js/dx.all.js"></script>
    <script src="/plugins/devextreme/js/aspnet/dx.aspnet.data.js"></script>
    <script src="/plugins/devextreme/js/aspnet/dx.aspnet.mvc.js"></script>
    <script src="/plugins/devextreme/devextreme-license.js"></script>
    @if (Model.ShouldIncludeLocalizationFile)
    {
    <script src="/plugins/devextreme/js/localization/dx.messages.@(Html.Raw(Model.LanguageCultureCode)).js"></script>
    }
}